# Staging Environment Variables
# Copy this file to .env.staging on your server

# Database
MONGO_USERNAME=voltuser
MONGO_PASSWORD=your-staging-mongo-password
MONGO_URI=mongodb://voltuser:your-staging-mongo-password@mongo:27017/volt_staging

# Redis
REDIS_PASSWORD=your-staging-redis-password
REDIS_URI=redis://:your-staging-redis-password@redis:6379

# JWT
JWT_SECRET=your-staging-jwt-secret-key

# API URLs (update with your staging domain)
CORS_ORIGIN=https://staging.volt.yourdomain.com
WS_ORIGIN=wss://staging-api.yourdomain.com
VITE_API_URL=https://staging-api.yourdomain.com
VITE_WS_URL=wss://staging-api.yourdomain.com

# App Configuration
VITE_APP_NAME=Volt (Staging)
VITE_BRAND_COLOR=#2563EB
GIN_MODE=debug